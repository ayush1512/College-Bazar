<%- layout('layouts/boilearplate') %>
<div>
    <div>
        <div>
            <% for(let image of product.fileToUpload){ %>
                <img src="<%= image.url %>" alt="Product Image">
            <% } %>
        </div>
        <div>
            <div>
                <h2><%= product.category %></h2>
            </div>
            <div>
                <h2><%= product.brand %></h2>
            </div>
            <div>
                <h2><%= product.title %></h2>
            </div>
            <div>
                <h2><%= product.description %></h2>
            </div>
            <div>
                <h3><%= product.price %></h3>
            </div>
            <div>
                <h2><%= product.location %></h2>
            </div>
            <div>
                <h2><%= product.phone %></h2>
            </div>
        </div>
    </div>
    <div>
        <!--List of same product with same cotegery-->
    </div>
</div>